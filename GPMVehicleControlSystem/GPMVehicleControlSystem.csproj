		</None>
		<None Update="src\sounds\traverse_right.mp3">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
		<None Update="wwwroot">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
	</ItemGroup>
	<ProjectExtensions>
		<VisualStudio>
			<UserProperties appsettings_1json__JsonSchema="" />
		</VisualStudio>
	</ProjectExtensions>

	<Target Name="AfterPublish" AfterTargets="Publish">
		<!-- 输出发布目录 -->
		<Message Text="Publish Directory: $(PublishDir)" Importance="high" />
		<WriteLinesToFile File="PublishOutputDirectory.txt" Lines="$(PublishDir)" Overwrite="true" />
	</Target>

	<Target Name="GenerateVersionJson_Complie" AfterTargets="Build">
		<Exec Command="powershell -ExecutionPolicy Bypass -File &quot;generate-version.ps1&quot; &quot;$(OutputDir).&quot; &quot;$(AssemblyVersion)&quot;" />
	</Target>

	<Target Name="GenerateVersionJson_Pub" AfterTargets="Publish">
		<Exec Command="powershell -ExecutionPolicy Bypass -File &quot;generate-version.ps1&quot; &quot;$(PublishDir).&quot; &quot;$(AssemblyVersion)&quot;" />
	</Target>

	<Target Name="CreatePathchZipFile_WithWWWROOT" AfterTargets="Publish">
		<Exec Command="powershell -ExecutionPolicy Bypass -File &quot;create-patch-zip-file.ps1&quot; &quot;$(PublishDir).&quot; &quot;true&quot;" />
	</Target>

	<Target Name="CreatePathchZipFile" AfterTargets="Publish">
		<Exec Command="powershell -ExecutionPolicy Bypass -File &quot;create-patch-zip-file.ps1&quot; &quot;$(PublishDir).&quot; &quot;false&quot;" />
	</Target>

	<Target Name="CheckGitStatusBeforePublish" BeforeTargets="Publish" Condition="'$(CheckGitClean)' == 'true'">
		<Exec Command="powershell -NoProfile -ExecutionPolicy Bypass -File CheckGitStatus.ps1" IgnoreExitCode="false" />
		<Error Condition="'$(MSBuildLastTaskResult)' == 'false'" Text="❌ Git 狀態檢查失敗，請先 commit 未提交的變更！" />
	</Target>
</Project>
































































































































































































































































































































































































































































































































































































































































